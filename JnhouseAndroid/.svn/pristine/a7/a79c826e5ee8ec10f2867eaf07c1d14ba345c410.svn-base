<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <jnhouse.topwellsoft.com.jnhouse_android.view.FixedGridView
        android:id="@+id/gv_channel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background_white"
        android:numColumns="3"
        android:scrollbars="none"
        android:stretchMode="columnWidth" />

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#EAEAEA"
        android:layout_marginTop="5dp"/>

    <include layout="@layout/tp_index_house_aide"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#EAEAEA"
        android:layout_marginTop="5dp"/>

    <TextView
        android:layout_width="match_parent"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_height="45dp"
        android:drawableRight="@drawable/index_zx_more"
        android:text="购房指南"
        android:textColor="@color/app_font_color_black"
        android:textSize="16dp"
        android:padding="10dp"
        android:gravity="center_vertical"
        android:background="@color/background_white"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_font_color_grey"/>


</LinearLayout>


