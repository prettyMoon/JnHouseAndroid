package jnhouse.topwellsoft.com.jnhouse_android.ui.houseloan;

public class Utils {
	public static double getLiLv(double lilv,int position) {
		switch (position) {
		case 0:
			lilv = lilv * 0.7;
			return lilv;
		case 1:
			lilv = lilv * 0.75;
			return lilv;
		case 2:
			lilv = lilv * 0.8;
			return lilv;
		case 3:
			lilv = lilv * 0.85;
			return lilv;
		case 4:
			lilv = lilv * 0.9;
			return lilv;
		case 5:
			lilv = lilv * 0.95;
			return lilv;
		case 6:
			lilv = lilv * 1;
			return lilv;
		case 7:
			lilv = lilv * 1.1;
			return lilv;
		case 8:
			lilv = lilv * 1.2;
			return lilv;
		case 9:
			lilv = lilv * 1.3;
			return lilv;
		default:
			return lilv;
		}

	}
	public static boolean isNull(String values) {
		if ((values != null) && (!"".equals(values))
				&& (!"null".equals(values))) {
			return false;
		}
		return true;
	}
}
