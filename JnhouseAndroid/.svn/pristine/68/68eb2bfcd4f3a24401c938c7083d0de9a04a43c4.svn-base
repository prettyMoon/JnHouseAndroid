<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:padding="10dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/mine_sell_thumb_iv"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/ic_launcher"/>
        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="@color/black"
                    android:textSize="15sp"
                    android:text="id"
                    android:id="@+id/mine_rent_id_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentRight="true"
                    android:textColor="@color/black"
                    android:textSize="15sp"
                    android:text="状态"
                    android:id="@+id/mine_rent_state_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
            <TextView
                android:textColor="@color/black"
                android:textSize="20sp"
                android:singleLine="true"
                android:text="委托标题"
                android:id="@+id/mine_rent_house_title_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="x"
                    android:id="@+id/mine_rent_house_room_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="室"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="x"
                    android:id="@+id/mine_rent_house_hall_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="厅"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="x"
                    android:id="@+id/mine_rent_house_toilet_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="卫"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="x"
                    android:id="@+id/mine_rent_house_kitchen_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="厨"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginLeft="10dp"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="x"
                    android:id="@+id/mine_rent_house_balcony_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="阳台"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginLeft="10dp"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="面积"
                    android:id="@+id/mine_rent_house_area_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="㎡"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <!--<TextView
                    android:layout_marginLeft="10dp"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="朝向"
                    android:id="@+id/mine_sell_house_toward_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />-->



            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="15sp"
                    android:text="小区名称"
                    android:id="@+id/mine_rent_borough_name_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_alignParentRight="true"
                        android:textSize="18sp"
                        android:textColor="@color/tp_index_second_house_title"
                        android:text="单位"
                        android:id="@+id/mine_rent_house_price_unit_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_toLeftOf="@id/mine_rent_house_price_unit_tv"
                        android:textSize="18sp"
                        android:textColor="@color/tp_index_second_house_title"
                        android:text="价格"
                        android:id="@+id/mine_rent_house_price_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/layout_del"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <CheckBox
            android:id="@+id/rentlist_checkbox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</RelativeLayout>